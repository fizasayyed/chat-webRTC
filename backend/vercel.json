{
"version": 2,
"name": "sneakspeak-backend",
"builds": [
{
"src": "index.js",
"use": "@vercel/node"
}
],
"routes": [
{
"src": "/(.*)",
"dest": "/index.js"
}
],
"headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "https://sneakspeak.vercel.app" }
      ]
    }
  ]
}